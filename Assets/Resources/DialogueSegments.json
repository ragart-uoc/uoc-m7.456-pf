[
    {
        "id": 1,
        "speaker": "Character",
        "content": "Yes?",
        "nextSegment": 2,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 2,
        "speaker": "Character",
        "content": "...",
        "nextSegment": 3,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 3,
        "speaker": "Character",
        "content": "What do you mean, she's dead? Suicide!?",
        "nextSegment": 4,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 4,
        "speaker": "Character",
        "content": "No... My friend...\nHow could this happen?",
        "nextSegment": 5,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 5,
        "speaker": "Character",
        "content": "Why didn't I do anything?\nHow could I not realize what she was going through?",
        "nextSegment": 6,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 6,
        "speaker": "Character",
        "content": "What if I didn't want to realize? What if she was desperately asking for help?",
        "nextSegment": 7,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 7,
        "speaker": "Character",
        "content": "What if...?",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 8,
        "speaker": "Character",
        "content": "Where am I...?",
        "nextSegment": 9,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 9,
        "speaker": "Character",
        "content": "The phone call... my friend...",
        "nextSegment": 10,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 10,
        "speaker": "Character",
        "content": "I need to do something.",
        "nextSegment": 11,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 11,
        "speaker": "Character",
        "content": "But what?",
        "nextSegment": 12,
        "bridgeOpen": 1,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 12,
        "speaker": "Character",
        "content": "A bridge. I wonder what's on the other side.",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 13,
        "speaker": "Character",
        "content": "Not again... What did I do wrong?",
        "nextSegment": null,
        "bridgeOpen": 1,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 14,
        "speaker": "Character",
        "content": "Wait... That's new.",
        "nextSegment": null,
        "bridgeOpen": 9,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 15,
        "speaker": "Character",
        "content": "Hey... What's going on?",
        "nextSegment": 16,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 16,
        "speaker": "Character",
        "content": "It's... me?",
        "nextSegment": 17,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 17,
        "speaker": "???",
        "content": "NOT EXACTLY.",
        "nextSegment": 18,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 18,
        "speaker": "???",
        "content": "I AM THE MECHANISM OF YOUR BRAIN THAT PROTECTS YOU FROM REALITY.",
        "nextSegment": 19,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 19,
        "speaker": "Character",
        "content": "Protect... me? Who are you? Where is my friend?",
        "nextSegment": 20,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 20,
        "speaker": "Logos",
        "content": "THERE IS NO FRIEND. THERE IS ONLY YOU. YOU'RE DYING AND I AM YOUR WAY OF COPING. I AM LOGOS.",
        "nextSegment": 21,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 21,
        "speaker": "Character",
        "content": "I don't understand...",
        "nextSegment": 22,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 22,
        "speaker": "Logos",
        "content": "THERE IS NO NEED TO UNDERSTAND.",
        "nextSegment": 23,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 23,
        "speaker": "Character",
        "content": "Then, all those deaths... It was all me? How have I really died?",
        "nextSegment": 24,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 24,
        "speaker": "Logos",
        "content": "DOES IT REALLY MATTER? ALL THAT MATTERS IS THE TRUTH. AND THE TRUTH IS THAT YOU ARE DYING. NOW IT'S TIME.",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 9,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 25,
        "speaker": "Friend",
        "content": "It's pretty clear that you're not interested in what I have to say. You won't stop interrupting me with a bunch of nonsense. This friendship is not going anywhere. Goodbye.",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 4,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 26,
        "speaker": "Character",
        "content": "Yes?",
        "nextSegment": 27,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 27,
        "speaker": "Character",
        "content": "She's dead...? And you don't know how?",
        "nextSegment": 28,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 28,
        "speaker": "Character",
        "content": "I shouldn't have left her go...",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 29,
        "speaker": "Friend",
        "content": "What...?",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 30,
        "speaker": "Character",
        "content": "Yes?",
        "nextSegment": 31,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 31,
        "speaker": "Character",
        "content": "Dead, yes. Suicide. I know... Does it matter? I just know.",
        "nextSegment": 32,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 32,
        "speaker": "Character",
        "content": "Let's try again.",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 1,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 33,
        "speaker": "Character",
        "content": "Yes?",
        "nextSegment": 34,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 34,
        "speaker": "Character",
        "content": "What do you mean, an accident? How? When?",
        "nextSegment": 35,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 35,
        "speaker": "Character",
        "content": "Not the right path. I'm sorry, my friend..",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 2,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 36,
        "speaker": "Character",
        "content": "Yes?",
        "nextSegment": 37,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 37,
        "speaker": "Character",
        "content": "Dead? How? She OD'd?",
        "nextSegment": 38,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 38,
        "speaker": "Character",
        "content": "This is not the way. She needs her family.",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 3,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 39,
        "speaker": "Character",
        "content": "Yes?",
        "nextSegment": 40,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 40,
        "speaker": "Character",
        "content": "So, she feels alright? Better than ever? I'm glad. I'm happy for her.",
        "nextSegment": 41,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 41,
        "speaker": "Character",
        "content": "I think this is it. She'll die when she's old, cozy in her bed. Not a minute before.",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 5,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 42,
        "speaker": "Character",
        "content": "Yes?",
        "nextSegment": 43,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 43,
        "speaker": "Character",
        "content": "Oh, dear. She's dead? I didn't know she was so ill...",
        "nextSegment": 44,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 44,
        "speaker": "Character",
        "content": "That didn't go as I expected...",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 6,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 45,
        "speaker": "Character",
        "content": "Yes?",
        "nextSegment": 46,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 46,
        "speaker": "Character",
        "content": "What!? She's dead? Her boyfriend killed her?",
        "nextSegment": 47,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 47,
        "speaker": "Character",
        "content": "I'm sorry, my friend. I'm sorry.",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 7,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 48,
        "speaker": "Character",
        "content": "Yes?",
        "nextSegment": 49,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 49,
        "speaker": "Character",
        "content": "What do you mean she drowned? How could that happen?",
        "nextSegment": 50,
        "bridgeOpen": null,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 50,
        "speaker": "Character",
        "content": "Crying by the river because of me. That's how she died...",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 51,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 51,
        "speaker": "Friend",
        "content": "What will you do for our friendship?",
        "nextSegment": 52,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 52,
        "speaker": "Friend",
        "content": "Have you ever thought about that?",
        "nextSegment": 53,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "fear",
                "nextSegment": 55
            }
        ]
    },
    {
        "id": 53,
        "speaker": "Friend",
        "content": "I think about it everyday.",
        "nextSegment": 54,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 54,
        "speaker": "Friend",
        "content": "What are we without each other?",
        "nextSegment": null,
        "bridgeOpen": 2,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 55,
        "speaker": "Friend",
        "content": "Huh? What are you scared about? Losing our friendship?",
        "nextSegment": 53,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "doubt",
                "nextSegment": 56
            }
        ]
    },
    {
        "id": 56,
        "speaker": "Friend",
        "content": "I won't ever hesitate about us. Don't be afraid.",
        "nextSegment": null,
        "bridgeOpen": 6,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 57,
        "speaker": "Friend",
        "content": "Do you remember how we used to play here when we were little children?",
        "nextSegment": 58,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 58,
        "speaker": "Friend",
        "content": "I guess we are not children anymore. And everyone expects something else from us.",
        "nextSegment": 59,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 59,
        "speaker": "Friend",
        "content": "But I don't think I'm ready to be what they want me to be.",
        "nextSegment": 60,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "guilt",
                "nextSegment": 61
            }
        ]
    },
    {
        "id": 60,
        "speaker": "Friend",
        "content": "I fear that they won't accept me for who I am.",
        "nextSegment": null,
        "bridgeOpen": 3,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 61,
        "speaker": "Friend",
        "content": "Guilt? Why should I feel guilty?",
        "nextSegment": 60,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "myself",
                "nextSegment": 62
            }
        ]
    },
    {
        "id": 62,
        "speaker": "Friend",
        "content": "Oh, no! Don't be! You're not like the others. You're like… me.",
        "nextSegment": null,
        "bridgeOpen": 7,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 63,
        "speaker": "Friend",
        "content": "Look at the sights from here. This is probably the highest place in the city.",
        "nextSegment": 64,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 64,
        "speaker": "Friend",
        "content": "The fall from here… Phew. But also, so close…",
        "nextSegment": 65,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 65,
        "speaker": "Friend",
        "content": "It will be so easy… Just a little step, and…",
        "nextSegment": 66,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "happiness",
                "nextSegment": 67
            }
        ]
    },
    {
        "id": 66,
        "speaker": "Friend",
        "content": "I'm kidding. What would you do without me? You're my family.",
        "nextSegment": null,
        "bridgeOpen": 4,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 67,
        "speaker": "Friend",
        "content": "I won't call it happiness… It's… something else. Like liberation. Like being released from all the guilt of being.",
        "nextSegment": 68,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 68,
        "speaker": "Friend",
        "content": "You know what? I'm going to stay here a little more…",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 6,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 69,
        "speaker": "Friend",
        "content": "I hate you! I don't… I…",
        "nextSegment": 70,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 70,
        "speaker": "Friend",
        "content": "Sorry. I'm just… No, it doesn't matter. It's not important.",
        "nextSegment": 71,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 71,
        "speaker": "Friend",
        "content": "You know what I like? Serendipity. That's a world like no other.",
        "nextSegment": 72,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 72,
        "speaker": "Friend",
        "content": "I'm not drunk… I'm just… happy. Well, not really… I don't know. What is happening to me?",
        "nextSegment": null,
        "bridgeOpen": 5,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "loneliness",
                "nextSegment": 73
            }
        ]
    },
    {
        "id": 73,
        "speaker": "Friend",
        "content": "You think I'm lonely? You think that's my biggest problem? What would you know? I have lots of people around me.",
        "nextSegment": null,
        "bridgeOpen": 5,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "love",
                "nextSegment": 74
            },
            {
                "word": "family",
                "nextSegment": 75
            }
        ]
    },
    {
        "id": 74,
        "speaker": "Friend",
        "content": "No one loves me, that's what you mean? You don't care about my health, you don't care about my hobbies… No, you care about my sex life. How typical. I'm leaving.",
        "nextSegment": null,
        "bridgeOpen": 8,
        "ending": null,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 75,
        "speaker": "Friend",
        "content": "My family abandoned me. That's what you mean. I know. You don't have to tell me. I'm leaving.",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 3,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 76,
        "speaker": "Friend",
        "content": "Don't follow me! I told you I need to be alone!",
        "nextSegment": 77,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 77,
        "speaker": "Friend",
        "content": "Fuck love. Fuck feelings. They're just the opium to keep us addicted to our shitty lives.",
        "nextSegment": 78,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 78,
        "speaker": "Friend",
        "content": "There nothing left for me. Not here, not anywhere.",
        "nextSegment": 79,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "life",
                "nextSegment": 80
            }
        ]
    },
    {
        "id": 79,
        "speaker": "Friend",
        "content": "I promise you, I will kill myself if I don't escape this hellhole.",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 1,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 80,
        "speaker": "Friend",
        "content": "Living? Why should I keep doing that? Do yourself a favor and leave life out of this.",
        "nextSegment": 79,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "time",
                "nextSegment": 81
            }
        ]
    },
    {
        "id": 81,
        "speaker": "Friend",
        "content": "Time… I have all the time in the world. Maybe you're right. I don't have to waste my time anymore. I'm going to live my life.",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 2,
        "interactable": false,
        "usableWords": []
    },
    {
        "id": 82,
        "speaker": "Friend",
        "content": "The leaves are falling early this year.",
        "nextSegment": 83,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 83,
        "speaker": "Friend",
        "content": "Ah, it's so beautiful!",
        "nextSegment": 84,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "yourself",
                "nextSegment": 88
            }
        ]
    },
    {
        "id": 84,
        "speaker": "Friend",
        "content": "This is my favorite place in the world. It's just me… and my loneliness.",
        "nextSegment": 85,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "serendipity",
                "nextSegment": 86
            }
        ]
    },
    {
        "id": 85,
        "speaker": "Friend",
        "content": "I don't know why I invited you to come. This is mine. Maybe I just wanted to share it with you while I can…",
        "nextSegment": null,
        "bridgeOpen": null,
        "ending": 1,
        "interactable": true,
        "usableWords": []
    },
    {
        "id": 86,
        "speaker": "Friend",
        "content": "Ha ha ha! Yes, I love that word. You remembered. That's what you do. You care about what's important.",
        "nextSegment": 85,
        "bridgeOpen": null,
        "ending": null,
        "interactable": true,
        "usableWords": [
            {
                "word": "friendship",
                "nextSegment": 87
            }
        ]
    },
    {
